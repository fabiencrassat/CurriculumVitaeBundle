{# Nimbusletruand/CurriculumVitaeBundle/Resources/views/Default/OryzoneBoilerplate.html.twig #}

{% extends "OryzoneBoilerplateBundle::html5.html.twig" %}

{% block head_title %}CV {{ cvxmlfile|title }}{% endblock %}

{% block head_css %}
{{ parent() }}
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="{% block body_css_bootstrap_onlineSrc %}//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css{% endblock %}" id="body_css_bootstrap">
{% stylesheets '@NimbusletruandCurriculumVitaeBundle/Resources/public/css/OryzoneBoilerplate.template.css' %}
<link rel="stylesheet" href="{{ asset_url }}">
{% endstylesheets %}
{% endblock %}

{% block body_js %}
{{ parent() }}
<!-- Latest compiled and minified JavaScript -->
<script src="{% block body_js_bootstrap_onlineSrc %}//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js{% endblock %}"></script>
<script>
if ($.fn.button == undefined) {
    document.write('<script type="text/javascript" src="{% block body_js_bootstrap_offlineSrc %}{% javascripts '@NimbusletruandCurriculumVitaeBundle/Resources/offline/js/bootstrap.min.js' %}{{ asset_url }}{% endjavascripts %}{% endblock %}"><\/script>');
    document.write('<link rel="stylesheet" href="{% block body_css_bootstrap_offlineSrc %}{% stylesheets '@NimbusletruandCurriculumVitaeBundle/Resources/offline/css/bootstrap.min.css' %}{{ asset_url }}{% endstylesheets %}{% endblock %}" id="body_css_bootstrap_offlineSrc">');
    $('document').ready(function(){
        $('link#body_css_bootstrap_offlineSrc').appendTo('link#body_css_bootstrap')
    })
};
</script>
{% endblock %}

{% block body_container_main %}
<nav class="navbar navbar-default navbar-fixed-top navbar-inverse" role="navigation">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <a href="#" class="navbar-brand">Curriculum Vitae</a>
    </div>
    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
            {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Nav/LanguagesList.html.twig' with {'template': 'OryzoneBoilerplate'} %}
            {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Nav/Anchors.html.twig' %}
        </ul>
    </div>
</nav>

<div id="CurriculumVitae" class="container">
    {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Includes/BusinessCard.html.twig' with {'template': 'OryzoneBoilerplate'} %}

    <div class="row">
        <div class="col-md-6">
            {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Includes/Identity.html.twig' with {'template': 'OryzoneBoilerplate'} %}
        </div>

        <div class="col-md-6">
            {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Includes/FollowMe.html.twig' with {'template': 'OryzoneBoilerplate'} %}
        </div>
    </div>

    {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Includes/Experiences.html.twig' with {'template': 'OryzoneBoilerplate'} %}
    {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Includes/Skills.html.twig' with {'template': 'OryzoneBoilerplate'} %}
    {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Includes/Educations.html.twig' with {'template': 'OryzoneBoilerplate'} %}
    {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Includes/LanguageSkills.html.twig' with {'template': 'OryzoneBoilerplate'} %}
    {% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Includes/Miscellaneous.html.twig' with {'template': 'OryzoneBoilerplate'} %}
</div>

{% include 'NimbusletruandCurriculumVitaeBundle:CurriculumVitae:Nav/TopButton.html.twig' with {'template': 'OryzoneBoilerplate'} %}
{% endblock %}