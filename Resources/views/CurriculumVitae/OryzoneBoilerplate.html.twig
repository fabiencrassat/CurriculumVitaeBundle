{# FabienCrassat/CurriculumVitaeBundle/Resources/views/CurriculumVitae/OryzoneBoilerplate.html.twig #}

{% extends "@FabienCrassatCurriculumVitae/OryzoneBoilerplate/html5.html.twig" %}

{% set bp_body_attributes = "data-spy='scroll' data-target='#bs-example-navbar-collapse-1'" %}

{% block head_title %}CV {{ cvxmlfile|title }}{% endblock %}

{% block head_css %}
{{ parent() }}
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="{% block body_css_bootstrap_onlineSrc %}//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css{% endblock %}" id="body_css_bootstrap">
<link href="{{ asset('bundles/fabiencrassatcurriculumvitae/css/OryzoneBoilerplate.template.css') }}" rel="stylesheet" />
{% endblock %}

{% block body_js %}
{{ parent() }}
<!-- Latest compiled and minified JavaScript -->
<script src="{% block body_js_bootstrap_onlineSrc %}//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js{% endblock %}"></script>
<script>
if ($.fn.button == undefined) {
    document.write('<script type="text/javascript" src="{% block body_js_bootstrap_offlineSrc %}{{ asset('fabiencrassatcurriculumvitae/offline/js/bootstrap.min.js') }}{% endblock %}"><\/script>');
    document.write('<link rel="stylesheet" href="{% block body_css_bootstrap_offlineSrc %}{{ asset('fabiencrassatcurriculumvitae/offline/css/bootstrap.min.css') }}{% endblock %}" id="body_css_bootstrap_offlineSrc">');
    $('document').ready(function(){
        $('link#body_css_bootstrap_offlineSrc').appendTo('link#body_css_bootstrap')
    })
};
</script>
{% if hasSecureDisplayBundle %}
  <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
  <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
  <script src="{{ asset('bundles/netinfluencesecuredisplay/js/display.js') }}"></script>
{% endif %}
{% endblock %}

{% block body_container_main %}
{% include '@FabienCrassatCurriculumVitae/CurriculumVitae/navigation.html.twig' with {'template': 'OryzoneBoilerplate'} %}
{% include '@FabienCrassatCurriculumVitae/CurriculumVitae/body.html.twig' with {'template': 'OryzoneBoilerplate'} %}
{% include '@FabienCrassatCurriculumVitae/CurriculumVitae/Nav/TopButton.html.twig' with {'template': 'OryzoneBoilerplate'} %}
{% endblock %}
