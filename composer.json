{
    "name": "fabiencrassat/curriculumvitae",
    "type": "symfony-bundle",
    "description": "Symfony CV bundle",
    "keywords": ["symfony2", "curriculum vitae", "xml", "cv"],
    "license": "MIT",
    "authors": [{
        "name": "Fabien Crassat",
        "email": "fabien@crassat.com"
    }],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/force-lowest": "=4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "suggest": {
        "oryzone/boilerplate-bundle": "Make the curriculum vitae beautiful",
        "knplabs/knp-snappy-bundle": "Export your curriculum vitae to PDF",
        "a5sys/pdf-bundle": "Export your curriculum vitae to PDF without proc_open",
        "netinfluence/secure-display-bundle": "Protect your email and phone number",
        "php": "PHP 5.7 is my website version"
    },
    "autoload": {
        "psr-4": {
            "FabienCrassat\\CurriculumVitaeBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FabienCrassat\\CurriculumVitaeBundle\\Test\\": "Tests/Fixtures/App/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symfony/phpunit-bridge": "^4.1"
    }
}
